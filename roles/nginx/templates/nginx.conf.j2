worker_processes 1;

events {
    worker_connections 1024:
}

http {
    include mime.types;
    default_type applaction/octet-stream;

    sendfile    on;
    keppalive_timeout 65;

    server {
        listen 80;
        server_name {{ nginx_svr_name }};

        location / {
            root html;
            index index.html index.htm;
        }
    }
}